<div id="container">

  <div id="articles-and-form-container">

    <div id="header">
        <%= link_to 'JOURNAL', articles_path %>
    </div>

    <div id="articles">
      <%= render partial: 'article', locals: { article: @article } %>
    </div>

  </div>

  <div id="archive-and-tags-container">

    <div id="archive">
      <%= render partial: 'articles/archive' %>
    </div>

    <div id="tag-box">
      <%= render partial: 'articles/tags' %>
    </div>

  </div>

  <div class="clear"></div>

</div>

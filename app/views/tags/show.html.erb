<div id="container">

  <div id="articles">

    <% @articles.each do |article| %>

      <%= render partial: 'articles/article', locals: { article: article } %>

    <% end %>

  </div>

  <div id="archive">

    <%= render partial: 'articles/archive' %>

  </div>

  <div class="clear"></div>

</div>
